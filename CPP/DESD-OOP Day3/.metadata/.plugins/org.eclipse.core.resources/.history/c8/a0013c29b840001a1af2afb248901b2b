#include <iostream>
using namespace std;

class constDemo
{
private:
	int n;
	const int c;
	mutable int m;

public:
	constDemo():c(5) //default constructor
	{
		n=100;
		m=300;
	}

/*
	constDemo(int nn,int cc,int mm):c(cc)
		{
			this->n=nn;
			this->m=mm;
		}
*/


	constDemo(int nn,int cc,int mm):n(nn),c(cc),m(mm)
	{ }


};

int main()
{

	return 0;
}
