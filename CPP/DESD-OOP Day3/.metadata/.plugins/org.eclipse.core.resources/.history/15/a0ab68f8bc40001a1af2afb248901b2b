#include <iostream>
using namespace std;
class Point
{
private:
	int x;
	int y;
public:
	Point()
	{
		this->x=0;
		this->y=0;
	}

	Point(int x,int y)
		{
			this->x=x;
			this->y=y;
		}

	void sum();
};

void Point::sum()
{
	int res=this->x+this->y;
}

int main()
{
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
