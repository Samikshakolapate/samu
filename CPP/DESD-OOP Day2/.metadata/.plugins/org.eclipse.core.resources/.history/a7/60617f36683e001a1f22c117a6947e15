#include <iostream>
using namespace std;

class Complex
{
private:
	int real;
	int imag;
public:
	Complex()
	{
		real=0;
		imag=0;
	}

	Complex(int real , int imag)
		{
			this->real=real;
			this->imag=imag;
		}

};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
