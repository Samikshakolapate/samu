#include <iostream>
using namespace std;
class Complex
{
private:
	int real;
	int imag;
public:
	//constructor
	Complex()
	{
		this->real=0;
		this->imag=0;
	}
	Complex(int real,int imag)
	{
		this->real=real;
		this->imag=imag;
	}
	~Complex()
	{
		cout<<"Inside Desctructor";
	}



}; //end of class


int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
